<div class="full-container">
  <div class="provider-name-div">
    <h1><%= @provider.name %></h1>
  </div>
  <div class="provider-name-div">
    <h3>Patient List:</h3>
        <ul aria-labelledby="userList">
          <% @users.each do |user| %>
            <li>
              <a href="/provider_users_dashboard?user_id=<%= user.id %>"><%= user.first_name %> <%= user.last_name %></a>
            </li>
        <% end %>
      </ul>
  </div>
</div>
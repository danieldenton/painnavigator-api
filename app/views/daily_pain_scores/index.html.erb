<p id="notice"><%= notice %></p>

<h1>Daily Pain Scores</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Daily scores</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @daily_pain_scores.each do |daily_pain_score| %>
      <tr>
        <td><%= daily_pain_score.user_id %></td>
        <td><%= daily_pain_score.daily_scores %></td>
        <td><%= link_to 'Show', daily_pain_score %></td>
        <td><%= link_to 'Edit', edit_daily_pain_score_path(daily_pain_score) %></td>
        <td><%= link_to 'Destroy', daily_pain_score, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Daily Pain Score', new_daily_pain_score_path %>
